# RAW Data Tier definition
#
#####################################################################

block RAWEventContent = {
  untracked vstring outputCommands = {
     "drop *",
     "keep  FEDRawDataCollection_rawDataCollector_*_*",
     "keep  FEDRawDataCollection_source_*_*"
  }
}

replace RAWEventContent.outputCommands += L1TriggerRAW.outputCommands 
replace RAWEventContent.outputCommands += HLTriggerRAW.outputCommands 


